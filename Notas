
PYTHONPATH=$PYTHONPATH:/home/proyecto/prefix/default/lib/python2.7/dist-packages
export PYTHONPATH

LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/home/proyecto/prefix/default/lib
export LD_LIBRARY_PATH

PATH=$PATH:/home/proyecto/prefix/default/bin
export PATH

